{
  "name": "puppet-strings-open-app",
  "version": "0.0.0",
  "description": "An extension to puppet-strings for compiling and opening a web application in Chrome",
  "keywords": [
    "chrome",
    "bundle",
    "compile",
    "test"
  ],
  "homepage": "https://github.com/vinsonchuong/puppet-strings-open-app",
  "bugs": "https://github.com/vinsonchuong/puppet-strings-open-app/issues",
  "license": "MIT",
  "author": "Vinson Chuong <vinsonchuong@gmail.com>",
  "repository": "vinsonchuong/puppet-strings-open-app",
  "scripts": {
    "prepare": "flow-typed update --skip",
    "test": "standard -v --fix && flow && ava -v",
    "release": "semantic-release",
    "prepack": "build-esm"
  },
  "dependencies": {
    "get-port": "^5.0.0",
    "parcel-bundler": "^1.12.3",
    "puppet-strings": "^2.4.1",
    "tempy": "^0.3.0"
  },
  "devDependencies": {
    "ava": "^1.4.1",
    "build-esm": "^4.2.1",
    "flow-bin": "^0.97.0",
    "flow-typed": "^2.5.1",
    "overdub": "^2.3.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "semantic-release": "^15.13.3",
    "standard-esnext": "^5.3.0"
  },
  "ava": {
    "require": "overdub/register",
    "babel": {
      "testOptions": {
        "extends": "overdub/babel"
      }
    }
  }
}

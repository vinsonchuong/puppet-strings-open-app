{
  "name": "puppet-strings-open-app",
  "version": "0.0.0",
  "description": "An extension to puppet-strings for compiling and opening a web application in a browser",
  "keywords": [
    "chrome",
    "bundle",
    "compile",
    "test"
  ],
  "homepage": "https://github.com/vinsonchuong/puppet-strings-open-app",
  "bugs": "https://github.com/vinsonchuong/puppet-strings-open-app/issues",
  "license": "MIT",
  "author": "Vinson Chuong <vinsonchuong@gmail.com>",
  "type": "module",
  "repository": "vinsonchuong/puppet-strings-open-app",
  "scripts": {
    "test": "xo && ava",
    "release": "semantic-release"
  },
  "dependencies": {
    "get-port": "^6.1.2",
    "passing-notes": "^6.5.7",
    "passing-notes-ui": "^1.2.1",
    "puppet-strings": "^4.1.3",
    "puppet-strings-chrome": "^2.0.3"
  },
  "devDependencies": {
    "ava": "^5.2.0",
    "ava-patterns": "^3.2.0",
    "htm": "^3.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "semantic-release": "^20.1.1",
    "xo": "^0.53.1"
  },
  "ava": {
    "verbose": true,
    "timeout": "10m"
  },
  "xo": {
    "prettier": true,
    "space": 2,
    "semicolon": false,
    "rules": {
      "n/file-extension-in-import": "off",
      "node/file-extension-in-import": "off"
    }
  }
}
